            android:background="#808080" />


        <androidx.cardview.widget.CardView
            android:id="@+id/crawl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:cardBackgroundColor="@color/white"
            android:background="@color/white"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="20dp"
            android:layout_marginRight="20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:orientation="vertical"
                    android:layout_marginLeft="20dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="10dp"
                        android:text="Front Crawl or Freestyle "
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        android:textStyle="bold"/>

                    <View
                        android:id="@+id/headerCrawl"
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:layout_marginRight="30dp"
                        android:background="#808080" />


                    <pl.droidsonroids.gif.GifImageView
                        android:id="@+id/gifCrawl"
                        android:layout_width="wrap_content"
                        android:layout_height="150dp"
                        android:layout_marginTop="10dp"
                        android:src="@drawable/freestyle"
                        android:layout_gravity="center"/>

                    <TextView
                        android:id="@+id/contentCrawl"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/glacialindifferenceregular"
                        android:justificationMode="inter_word"
                        android:padding="15dp"
                        android:layout_gravity="center"
                        android:text="@string/Crawl"
                        android:textColor="@color/black"
                        android:textSize="16sp" />


                </LinearLayout>

            </LinearLayout>
        </androidx.cardview.widget.CardView>











    </LinearLayout>


</ScrollView>